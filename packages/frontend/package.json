    "@types/react-router-dom": "^5.1.5",
    "@types/validator": "^13.7.3",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "globals": "^15.1.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.7.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.14.2",
    "comlink": "^4.3.0",
    "dayjs": "^1.11.7",
    "dompurify": "^3.0.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "highlight.js": "^11.9.0",
    "jsdom": "^23.0.1",
    "jwt-decode": "^4.0.0",
    "marked": "^11.0.1",
    "marked-mangle": "^1.1.0",
    "normalize.css": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.6.1",
    "recoil": "^0.7.6",
    "validator": "^13.7.0",
    "vite": "^6.0.0",
    "vite-plugin-pwa": "^0.17.4"
  }
