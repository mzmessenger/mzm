    "@vitejs/plugin-react": "^2.2.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "highlight.js": "^11.6.0",
    "jsdom": "^20.0.2",
    "marked": "^4.2.2",
