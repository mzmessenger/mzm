    "dompurify": "^2.3.8",
    "highlight.js": "^11.5.1",
    "marked": "^4.0.16",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "validator": "^13.7.0"
