    "bunyan": "^1.8.14",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.0",
    "image-size": "^1.0.2",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.11.0",
    "multer": "^1.4.2",
    "node-schedule": "^2.1.0",
    "undici": "^5.12.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
